@model IEnumerable<StudenteDto>;

@{
}

<div class="card">
	<div class="card-header">Risultati:</div>

	<div class="card-body">

		<table class="table table-striped">
			<tr>
				<th>Nome</th>
				<th>Cognome</th>
				<th>Codice fiscale</th>
				<th>Data di nascita</th>
				<th>Corso Id</th>
			</tr>
			@foreach (var item in Model)
			{
				<tr>
					<td>@item.Nome</td>
					<td>@item.Cognome</td>
					<td>@item.CodiceFiscale</td>
					<td>@item.DataDiNascita</td>
					<td>@item.CorsoId</td>
				</tr>
			}
		</table>

	</div>
</div>
